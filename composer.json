{
  "name": "pdir/theme-odd-bundle",
  "type":"contao-bundle",
  "description": "theme odd bundle for Contao 4",
  "authors": [
    {
      "name": "Webdesign fÃ¼r Dresden",
      "homepage": "https://pdir.de",
      "email" : "develop@pdir.de",
      "role" : "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "contao/core-bundle": "^4.4",
    "contao-bootstrap/bundle": "~2.0",
    "madeyourday/contao-rocksolid-frontend-helper": "~2.0",
    "terminal42/contao-folderpage": "^2.0",
    "pdir/contao-theme-helper-bundle": "^1.0",
    "contao-themes-net/theme-components-bundle": "^1.1"
  },
  "conflict": {
    "contao/core": "*"
  },
  "autoload": {
    "psr-4": {
      "Pdir\\ThemeOddBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Pdir\\ThemeOddBundle\\ContaoManagerPlugin"
  }
}